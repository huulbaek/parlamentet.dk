generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model RefactorLog {
  OperationKey String @id(map: "PK____Refact__D3AEFFDBAD8CBA18") @db.UniqueIdentifier

  @@map("__RefactorLog")
}

model Afstemning {
  id              Int      @id(map: "PK_Afstemning")
  nummer          Int
  konklusion      String?  @db.NVarChar(Max)
  vedtaget        Boolean
  kommentar       String?  @db.NVarChar(Max)
  moedeid         Int      @map("mødeid")
  typeid          Int
  sagstrinid      Int?
  opdateringsdato DateTime @db.DateTime
}

model Afstemningstype {
  id              Int      @id(map: "PK_Afstemningstype")
  type            String   @db.NVarChar(Max)
  opdateringsdato DateTime @db.DateTime
}

model Aktoer {
  id              Int       @id(map: "PK_Aktør")
