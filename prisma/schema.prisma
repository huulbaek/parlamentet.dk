generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Quiz {
  id Int        @id @default(autoincrement())
  createdAt     DateTime @default(now())
  quizResults   QuizResults[]
}

model QuizResults {
  id Int        @id @default(autoincrement())
  initials      String
  color         String?
  logo          String?
  agreements    Int
  disagreements Int
  quizId        Int
  quiz          Quiz @relation(fields: [quizId], references: [id])
  createdAt     DateTime @default(now())
}
